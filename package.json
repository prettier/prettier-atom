{
  "name": "prettier-atom",
  "main": "./lib/prettier.js",
  "version": "0.0.6",
  "description": "Format file contents using prettier",
  "keywords": [
    "javascript",
    "formatter"
  ],
  "activationCommands": [],
  "repository": "https://github.com/jlongster/prettier-atom",
  "license": "MIT",
  "engines": {
    "atom": ">=0.174.0 <2.0.0"
  },
  "dependencies": {
    "find-root": "^0.1.1",
    "prettier": "0.0.6"
  },
  "devDependencies": {
    "eslint": "^3.6.0",
    "eslint-config-airbnb-base": "^8.0.0",
    "eslint-plugin-import": "^1.16.0"
  },
  "scripts": {
    "lint": "eslint ."
  },
  "eslintConfig": {
    "rules": {
      "array-bracket-spacing": [
        2,
        "always"
      ],
      "no-console": 0,
      "import/no-unresolved": [
        2,
        {
          "ignore": [
            "atom"
          ]
        }
      ]
    },
    "extends": "airbnb-base",
    "globals": {
      "atom": "true"
    },
    "env": {
      "es6": true,
      "node": true
    }
  }
}
